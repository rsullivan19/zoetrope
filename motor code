int MotorLF = 10;
int MotorLB = 9;
int MotorS = analogRead(A0);

void setup()
{
  pinMode(MotorLF, OUTPUT);
  pinMode(MotorLB, OUTPUT);
  pinMode(MotorS, OUTPUT);
  Serial.begin(9600);
}

void loop() {
  digitalWrite(MotorLF, HIGH);
  digitalWrite(MotorLB, LOW);
  int MotorS = analogRead(A0);
  Serial.println(MotorS);
  analogWrite(MotorS, 250);
 
}
